<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="asia.getl.dmc_getl">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/getllogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/getllogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".splash"
            android:theme="@style/AppFullScreenTheme"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Home"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>
        <activity android:name=".Add_driver"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".activity_counselling"
            android:label="@string/title_activity_counselling"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>
        <activity android:name=".Show_list"
            android:screenOrientation="portrait"/>
        <activity android:name=".Driver_Detail"
            android:screenOrientation="portrait"/>

        <activity android:name=".Vcheck_list"
            android:screenOrientation="portrait"/>
        <activity android:name=".Vehicalhistoryshow"
            android:screenOrientation="portrait"/>

        <activity android:name=".Vehiclelist"
            android:screenOrientation="portrait"/>
        <activity android:name=".Update_driver"
            android:screenOrientation="portrait"/>
        <activity android:name=".Vcheck_list_nav"
            android:screenOrientation="portrait"/>




    </application>

</manifest>